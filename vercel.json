{
  "buildCommand": null,
  "outputDirectory": ".",
  "redirects": [
    { "source": "/admin-dashboard", "destination": "/admin-dashboard.html", "permanent": true },
    { "source": "/associate-portal", "destination": "/associate-portal.html", "permanent": true },
    { "source": "/cv-creator", "destination": "/cv-creator.html", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ]
}
